<%- include("partials/header") %>

<div class="box">
  <div class="listOfLists">
    <h3>Your Lists</h3>
    <% listOfLists.forEach(function(list) { %>
    <p class="listTitle"><%= list %>
      <a class="listLink" href="/home/<%= listOfLists[list] %>"><i class="fas fa-check"></i></a>
    </p>
    <% }); %>
    <form class="" action="/" method="post">
      <input class="css-input" type="text" name="newList" value="" placeholder="New List">
      <button class="submit-btn" type="submit" name="button"> + </button>
    </form>
  </div>
  <div class="listOfTodos">
    <h3>Current List</h3>
    <div class="toDos">
      
    </div>
  </div>
</div>


<%- include("partials/footer") %>
